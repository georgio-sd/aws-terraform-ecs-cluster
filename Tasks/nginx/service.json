[
  {
    "name": "${name}",
    "image": "${image}",
    "memory": ${memory},
    "mountPoints": [
      {
        "containerPath": "${containerPath}",
        "sourceVolume": "service-storage"
      }
    ],
    "portMappings": [
      {
        "containerPort": ${containerPort}
      }
    ]
  }
]
